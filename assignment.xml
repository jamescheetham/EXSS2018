<?xml version="1.0" ?>
<Configuration>
    <dataset type='control' id='1'>
        <file filename='walk_lin.txt' framerate='25' skip="6">
            <columns>
                <col joint='toe' colnum='1' type='position' plane='x'/>
                <col joint='toe' colnum='2' type='position' plane='y'/>
                <col joint="toe" colnum="3" type="velocity" plane="x"/>
                <col joint="toe" colnum="4" type="velocity" plane="y"/>
                <col joint="ankle" colnum="5" type="position" plane="x"/>
                <col joint="ankle" colnum="6" type="position" plane="y"/>
                <col joint="ankle" colnum="7" type="velocity" plane="x"/>
                <col joint="ankle" colnum="8" type="velocity" plane="y"/>
                <col joint="knee" colnum="9" type="position" plane="x"/>
                <col joint="knee" colnum="10" type="position" plane="y"/>
                <col joint="knee" colnum="11" type="velocity" plane="x"/>
                <col joint="knee" colnum="12" type="velocity" plane="y"/>
                <col joint="hip" colnum="13" type="position" plane="x"/>
                <col joint="hip" colnum="14" type="position" plane="y"/>
                <col joint="hip" colnum="15" type="velocity" plane="x"/>
                <col joint="hip" colnum="16" type="velocity" plane="y"/>
            </columns>
        </file>
        <file filename='walk_ang.txt' framerate="25" skip="6">
            <columns>
                <col joint='ankle' colnum='1' type='angle'/>
                <col joint="knee" colnum="2" type="angle"/>
            </columns>

        </file>
        <keypoints>
            <point name='HO' framenum='5'/>
            <point name='FF' framenum='7'/>
            <point name="HS1" framenum="5"/>
            <point name="TO" framenum="21"/>
            <point name="HS2" framenum="31"/>
        </keypoints>
    </dataset>
    <definitions>
        <phases>
            <phase name='Early Stance' start='HO' end="FF"/>
            <phase name="Stride" start="HS1" end="HS2"/>
        </phases>
    </definitions>
    <analysis>
        <graph file="ankle_position.png" phase='Early Stance' title="Graph Title" joint="ankle" normalise='true' x-axis="x position" y-axis="y position" x-axis-title="Phase" y-axis-title="Height (m)"/>
        <graph file="knee_position.png" phase="Stride" title="Knee Position during Stride" joint="knee" normalise="true" x-axis="x position" y-axis="y position" x-axis-title="X Position" y-axis-title="Y Position (m)"/>
