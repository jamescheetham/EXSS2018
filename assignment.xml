        <graph title="Another Graph" phase='Early Stance' joint="ankle" x-axis="x velocity" y-axis="framenum"/>
        <graph title="Joint Angle" phase='Early Stance' joint="ankle" x-axis="angle" y-axis="framenum"/>
        -->
    </analysis>
</Configuration>
